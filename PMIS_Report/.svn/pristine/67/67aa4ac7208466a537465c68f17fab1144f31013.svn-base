package com.tetrapak.metaclass;

import com.tetrapak.domain.cip.CIPMasterLine;
import com.tetrapak.domain.cip.CIPPhase;

/**
 * This stands for the cip realtime running
 * */
public class CIPRealtimeData {
    private CIPMasterLine cipMasterLine;
    private CIPPhase cipPhase;
    private int cipTypePlcId;
    private String cipTypeDesc;
    private double ftOut;
    private double ttOut;
    private double ctBack;
    private double ttBack;
    private int stepNum;

    public CIPMasterLine getCipMasterLine() {
	return cipMasterLine;
    }

    public void setCipMasterLine(CIPMasterLine cipMasterLine) {
	this.cipMasterLine = cipMasterLine;
    }

    public CIPPhase getCipPhase() {
	return cipPhase;
    }

    public void setCipPhase(CIPPhase cipPhase) {
	this.cipPhase = cipPhase;
    }

    public int getCipTypePlcId() {
	return cipTypePlcId;
    }

    public void setCipTypePlcId(int cipTypePlcId) {
	this.cipTypePlcId = cipTypePlcId;
    }

    public String getCipTypeDesc() {
	return cipTypeDesc;
    }

    public void setCipTypeDesc(String cipTypeDesc) {
	this.cipTypeDesc = cipTypeDesc;
    }

    public double getFtOut() {
	return ftOut;
    }

    public void setFtOut(double ftOut) {
	this.ftOut = ftOut;
    }

    public double getTtOut() {
	return ttOut;
    }

    public void setTtOut(double ttOut) {
	this.ttOut = ttOut;
    }

    public double getCtBack() {
	return ctBack;
    }

    public void setCtBack(double ctBack) {
	this.ctBack = ctBack;
    }

    public double getTtBack() {
	return ttBack;
    }

    public void setTtBack(double ttBack) {
	this.ttBack = ttBack;
    }

    public int getStepNum() {
	return stepNum;
    }

    public void setStepNum(int stepNum) {
	this.stepNum = stepNum;
    }

}
