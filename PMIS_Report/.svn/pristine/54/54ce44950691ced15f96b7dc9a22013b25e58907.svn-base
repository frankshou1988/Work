package com.tetrapak.metaclass;

import java.util.Date;

public class BPUReportPhaseSummary {
    private Integer phaseN;
    private String phaseDesc;
    private Date phaseStartDateTime;
    private Date phaseEndDateTime;
    private String phaseLastTime;

    public Integer getPhaseN() {
	return phaseN;
    }

    public void setPhaseN(Integer phaseN) {
	this.phaseN = phaseN;
    }

    public String getPhaseDesc() {
	return phaseDesc;
    }

    public void setPhaseDesc(String phaseDesc) {
	this.phaseDesc = phaseDesc;
    }

    public Date getPhaseStartDateTime() {
	return phaseStartDateTime;
    }

    public void setPhaseStartDateTime(Date phaseStartDateTime) {
	this.phaseStartDateTime = phaseStartDateTime;
    }

    public Date getPhaseEndDateTime() {
	return phaseEndDateTime;
    }

    public void setPhaseEndDateTime(Date phaseEndDateTime) {
	this.phaseEndDateTime = phaseEndDateTime;
    }

    public String getPhaseLastTime() {
	return phaseLastTime;
    }

    public void setPhaseLastTime(String phaseLastTime) {
	this.phaseLastTime = phaseLastTime;
    }

}
