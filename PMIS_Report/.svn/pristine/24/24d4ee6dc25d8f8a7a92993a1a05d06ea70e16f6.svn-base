package com.tetrapak.metaclass;

/**
 * This class stands for the edge section of a discrete tag.
 * ___________________________ | | | | _______| |_________
 * 
 * <--- Edge Section --->
 * 
 * _______ ___________ | | | | |_________________________| <---Reverse Edge
 * Section-->
 * */
public class EdgeSection {
    private String edgeStartDateTime;
    private String edgeEndDateTime;

    public EdgeSection() {
	super();
    }

    public EdgeSection(String edgeStartDateTime, String edgeEndDateTime) {
	super();
	this.edgeStartDateTime = edgeStartDateTime;
	this.edgeEndDateTime = edgeEndDateTime;
    }

    public String getEdgeStartDateTime() {
	return edgeStartDateTime;
    }

    public void setEdgeStartDateTime(String edgeStartDateTime) {
	this.edgeStartDateTime = edgeStartDateTime;
    }

    public String getEdgeEndDateTime() {
	return edgeEndDateTime;
    }

    public void setEdgeEndDateTime(String edgeEndDateTime) {
	this.edgeEndDateTime = edgeEndDateTime;
    }

}
