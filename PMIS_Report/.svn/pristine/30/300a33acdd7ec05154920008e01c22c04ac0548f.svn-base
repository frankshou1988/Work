<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator 1.0.2//EN"
  "http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">
<validators>
	<field name="bpuMachineId">
		<field-validator type="required">
			<message key="i18n.pkg.error.bpu.machine.id.required" />
		</field-validator>
	</field>
	<field name="analogTagIdList">
		<field-validator type="required">
			<message key="i18n.pkg.error.alarm.tag.id.required" />
		</field-validator>
	</field>
	<field name="queryStartDate">
		<field-validator type="requiredstring">
			<message key="i18n.pkg.error.query.start.date.required" />
		</field-validator>
	</field>
	<field name="queryEndDate">
		<field-validator type="requiredstring">
			<message key="i18n.pkg.error.query.end.date.required" />
		</field-validator>
	</field>
	<field name="queryInterval">
		<field-validator type="required">
			<message key="i18n.pkg.error.query.interval.required" />
		</field-validator>
	</field>
	<field name="queryIntervalUnit">
		<field-validator type="required">
			<message key="i18n.pkg.error.query.interval.unit.required" />
		</field-validator>
	</field>
</validators>
