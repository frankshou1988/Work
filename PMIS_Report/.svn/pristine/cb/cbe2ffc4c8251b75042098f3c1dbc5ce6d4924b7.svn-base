<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<%@ taglib uri="/struts-tags" prefix="s"%>
<div class="cf"></div>
<input type="hidden" name="locale" value="<s:property value="#session.WW_TRANS_I18N_LOCALE"/>" id="current-locale" />
<div id="loading-dialog" class="default-none-display">
	<p>
		<s:text name="i18n.query.loading" />
	</p>
	<p>
		<img src="<s:url value="/public/images/loading.gif"/>" alt="<s:text name="i18n.query.loading"/>" title="<s:text name="i18n.query.loading"/>" />
	</p>
</div>
<input type="hidden" value="<s:text name="i18n.delete.warning"/>" id="delete-warning-text" />
<div id="footer">
	<p style="text-align: center;font-size:12px;color:#aaa;">
		<s:text name="i18n.copyright" />
	</p>
</div>
</body>
</html>